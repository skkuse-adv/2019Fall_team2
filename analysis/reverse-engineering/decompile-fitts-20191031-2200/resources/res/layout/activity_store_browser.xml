    <androidx.core.widget.ContentLoadingProgressBar android:id="@+id/contentLoadingProgressBar" android:background="@color/gray1" android:layout_width="match_parent" android:layout_height="3dp" android:layout_alignParentBottom="true" app:layout_constraintBottom_toBottomOf="@+id/storeWebview" app:layout_constraintEnd_toEndOf="0" app:layout_constraintStart_toStartOf="0" style="?android:attr/progressBarStyleHorizontal"/>
    <LinearLayout android:gravity="center" android:orientation="vertical" android:id="@+id/layoutStoreInfoBubble" android:background="@drawable/img_talk_bubble_black" android:visibility="gone" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginBottom="7dp" android:layout_marginEnd="26dp" app:layout_constraintBottom_toTopOf="@+id/containerController" app:layout_constraintEnd_toEndOf="0">
        <LinearLayout android:orientation="horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content">
            <TextView android:textSize="13sp" android:textColor="#ffffff" android:gravity="center_horizontal" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="핏츠에서 바로 구매" android:fontFamily="@font/kopubdotum_pro_bold"/>
