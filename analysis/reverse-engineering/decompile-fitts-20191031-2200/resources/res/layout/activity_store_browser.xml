        <TextView android:textSize="15sp" android:textColor="#000000" android:gravity="center_horizontal" android:id="@+id/tvNonPartner1" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="14dp" android:text="핏츠 스토어에서는\10매일 새로운 신상을" android:lineSpacingExtra="-2dp" android:fontFamily="@font/kopubdotum_pro_bold" app:layout_constraintBottom_toTopOf="@+id/tvNonPartner2" app:layout_constraintEnd_toEndOf="@+id/viewNonPartnerLoading" app:layout_constraintStart_toStartOf="@+id/viewNonPartnerLoading" app:layout_constraintTop_toBottomOf="@+id/iconLoadingStoreBrowser"/>
        <TextView android:textSize="15sp" android:textColor="@color/point" android:gravity="center_horizontal" android:id="@+id/tvNonPartner2" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="4dp" android:text="마진없는 가격에!" android:fontFamily="@font/kopubdotum_pro_bold" app:layout_constraintBottom_toTopOf="@+id/linearNonPartnerText" app:layout_constraintEnd_toEndOf="@+id/viewNonPartnerLoading" app:layout_constraintStart_toStartOf="@+id/viewNonPartnerLoading" app:layout_constraintTop_toBottomOf="@+id/tvNonPartner1"/>
        <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:id="@+id/linearNonPartnerText" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginTop="14dp" app:layout_constraintBottom_toBottomOf="@+id/viewNonPartnerLoading" app:layout_constraintEnd_toEndOf="@+id/viewNonPartnerLoading" app:layout_constraintStart_toStartOf="@+id/viewNonPartnerLoading" app:layout_constraintTop_toBottomOf="@+id/tvNonPartner2">
