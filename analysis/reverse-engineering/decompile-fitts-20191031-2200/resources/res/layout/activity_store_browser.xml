            <TextView android:textSize="13sp" android:textColor="#000000" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="스토어탭에서 PICK해보세요" android:fontFamily="@font/kopubdotum_pro_medium"/>
        </LinearLayout>
